(= (range $K $N)
   (if (< $K $N)
       (superpose ($K (range (+ $K 1) $N)))
       (empty)))

!(let* (($k (range 0 inf))
        ($inp (readln!))
        ($quotedfunc (quote = $X $Y))
        ($compiled (if (= $inp (cdr-atom $quotedfunc))
                       (add-atom &self $inp)
                       (add-atom &self (= (! $k) $inp))))
        ($outp (! $k))
        ($printed (println! $outp)))
       (empty))
